        "from __future__ import print_function\n",
        "from robustcontrol.utils import poles, zeros, tf, mimotf\n",
        "\n",
        "s = tf([1, 0], [1])\n",
        "G =  1 / (s + 2) * mimotf([[s - 1, 4], [4.5, 2 * (s - 1)]])\n",
        "print('Poles: ', poles(G))\n",
        "print('Zeros: ', zeros(G))\n"
